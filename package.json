{
  "name": "node-libjq",
  "types": "./dist/addon.d.ts",
  "main": "./dist/addon.js",
  "exports": {
    ".": {
      "types": "./dist/addon.d.ts",
      "import": "./dist/addon.mjs",
      "default": "./dist/addon.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm run build:addon && pnpm run build:dist",
    "build:addon": "cmake-js build",
    "build:dist": "nanobundle build",
    "dev": "tsx src/addon.ts",
    "test": "glob -c \"node --loader tsx --test\" src/**/*.spec.ts"
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.1",
    "@types/bindings": "^1.5.1",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "glob": "^10.2.7",
    "nanobundle": "^1.6.0",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^7.2.1",
    "node-addon-api": "^6.1.0"
  }
}
